{
  "name": "@patomation/create-react-app",
  "version": "1.0.1",
  "description": "A simple react app template that is already ejected with webpack and no crap",
  "private": false,
  "main": "./scripts/create-react-app.js",
  "scripts": {
    "start": "concurrently \"npm:watch:server\" \"npm:start:server\" \"npm:start:client\"",
    "build": "run-s build:server build:client",
    "build:client": "webpack --mode production --config ./webpack.prod.config.js",
    "start:client": "webpack-dev-server --mode development --open --hot --config ./webpack.dev.config.js",
    "watch:server": "npm run build:server -- --watch",
    "build:server": "tsc --build src/server",
    "start:server": "nodemon build/server/index.js --watch build/server",
    "lint": "./node_modules/.bin/eslint **/*.{js,ts,tsx}",
    "test": "jest",
    "coverage": "npm run test -- --coverage",
    "create-react-app": "node ./scripts/create-react-app.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config commitlint.config.json",
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "dependencies": {
    "@patomation/ui": "2.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.13",
    "@types/node": "14.14.6",
    "@types/react": "16.9.49",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "apollo-server-express": "2.19.0",
    "babel-loader": "8.1.0",
    "class-validator": "0.12.2",
    "concurrently": "5.3.0",
    "cors": "2.8.5",
    "css-loader": "4.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "eslint": "7.9.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-standard": "4.0.1",
    "express": "4.17.1",
    "file-loader": "6.1.0",
    "graphql": "15.4.0",
    "html-webpack-plugin": "4.4.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jsdom-global": "3.0.2",
    "node-sass": "4.14.1",
    "nodemon": "2.0.6",
    "npm-run-all": "4.1.5",
    "sass-loader": "10.0.2",
    "sqlite3": "5.0.0",
    "style-loader": "1.2.1",
    "ts-jest": "26.3.0",
    "ts-loader": "8.0.3",
    "tsconfigs": "5.0.0",
    "type-graphql": "1.1.0",
    "typeorm": "0.2.28",
    "typescript": "4.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patomation/react-webpack-boilerplate.git"
  },
