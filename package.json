{
  "name": "@patomation/create-react-app",
  "version": "1.0.1",
  "description": "A simple react app template that is already ejected with webpack and no crap",
  "private": false,
  "main": "./scripts/create-react-app.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot --config ./webpack.dev.config.js",
    "build": "webpack --mode production --config ./webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint **/*.{js,ts,tsx}",
    "test": "jest",
    "coverage": "npm run test -- --coverage",
    "create-react-app": "node ./scripts/create-react-app.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.1",
    "@types/react": "17.0.19",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
    "babel-loader": "8.2.2",
    "css-loader": "6.2.0",
    "enzyme": "3.11.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "4.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jsdom-global": "3.0.2",
    "node-sass": "7.0.0",
    "pretty-quick": "3.1.1",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "tsconfigs": "5.0.0",
    "typescript": "4.3.5",
    "url-loader": "4.1.1",
    "webpack": "5.51.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.0.0",
    "webpack-merge": "5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patomation/react-webpack-boilerplate.git"
  },
  "author": "",
