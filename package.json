{
  "name": "@patomation/create-react-app",
  "version": "1.0.1",
  "description": "A simple react app template that is already ejected with webpack and no crap",
  "private": false,
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot --config ./webpack.dev.config.js",
    "build": "webpack --mode production --config ./webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint **/*.{js,ts,tsx}",
    "test": "jest",
    "coverage": "npm run test -- --coverage",
    "create-react-app": "node ./scripts/create-react-app.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/jest": "27.0.1",
    "@types/node": "18.11.18",
    "@types/react": "17.0.19",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
    "babel-loader": "8.2.2",
    "css-loader": "6.2.0",
    "enzyme": "3.11.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "4.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "8.0.3",
    "jest": "27.0.6",
    "jsdom-global": "3.0.2",
    "pretty-quick": "3.1.3",
    "style-loader": "3.2.1",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "tsconfigs": "5.0.0",
    "typescript": "4.9.4",
    "url-loader": "4.1.1",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0",
    "ava": "^5.1.1",
    "concurrently": "7.6.0",
    "nyc": "15.1.0",
    "onchange": "7.1.0",
    "prettier": "2.8.3",
    "standard-version": "^9.5.0",
    "wait-on": "7.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/patomation/react-boilerplate.git"
  },
  "author": "Patrick Kelly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patomation/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/patomation/react-webpack-boilerplate#readme",
  "public": true,
  "types": "build/index.d.ts",
  "files": [
    "build",
    "bin"
  ],
  "keywords": [
    "typescript"
  ],
  "ava": {
    "files": [
      "build/**/*.test.js"
    ]
  }
}
